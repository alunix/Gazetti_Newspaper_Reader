<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="TransparentActionBar" parent="@style/Widget.AppCompat.ActionBar">
        <item name="android:background">@drawable/actionbar_transparent_background</item>
    </style>

    <style name="TransparentActionBarText" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="dialogBoxText" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="ButtonAppTheme" parent="android:Widget.Holo.Light.Button">
        <item name="android:background">@drawable/apptheme_btn_default_holo_light</item>
    </style>

    <style name="ImageButtonAppTheme" parent="android:Widget.Holo.Light.ImageButton">
        <item name="android:background">@drawable/apptheme_btn_default_holo_light</item>
    </style>

    <style name="ExpListDarkTheme" parent="android:Widget.Holo.ExpandableListView"></style>

</resources>